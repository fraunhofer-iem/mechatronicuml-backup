<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
      <editpartProvider
            class="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.custom.providers.CustomPlatformEditPartProvider">
         <Priority
               name="Low">
         </Priority>
      </editpartProvider>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.common.ui.services.parserProviders">
      <ParserProvider
            class="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.custom.providers.CustomPlatformParserProvider">
         <Priority
               name="Low">
         </Priority>
      </ParserProvider>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.custom.objectContribution1"
            objectClass="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.edit.parts.BusEditPart">
         <action
               class="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.custom.dialog.OpenProtocolDialog"
               id="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.custom.action1"
               label="Choose Bus Protocol">
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.custom.objectContribution2"
            objectClass="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.edit.parts.LinkEditPart">
         <action
               class="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.custom.dialog.OpenProtocolDialog"
               id="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.custom.action2"
               label="Choose Link Protocol">
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.custom.objectContribution2"
            objectClass="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.edit.parts.Link2EditPart">
         <action
               class="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.custom.dialog.OpenProtocolDialog"
               id="de.uni_paderborn.fujaba.muml.hardware.platform.diagram.custom.action2"
               label="Choose Link Protocol">
         </action>
      </objectContribution>
   </extension>

</plugin>
