      <?gmfgen generated="true"?>
      <ParserProvider class="de.uni_paderborn.fujaba.muml.component.diagram.custom.providers.CustomStructuredcomponentParserProvider">
         <Priority name="Low"/>
      </ParserProvider>
   </extension>
	-->
</plugin>
