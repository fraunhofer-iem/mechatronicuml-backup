       <propertyEditor
             factory="de.fujaba.properties.properties.properties.editor.ComboBoxPropertyEditorEditor$Factory"
             type="de.fujaba.properties.ComboBoxPropertyEditor">
       </propertyEditor>
    
       <propertyEditor
