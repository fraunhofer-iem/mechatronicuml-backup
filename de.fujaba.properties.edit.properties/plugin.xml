       <propertyEditor
             factory="de.fujaba.properties.properties.properties.editor.ObjectPropertyEditorEditor$Factory"
             type="de.fujaba.properties.ObjectPropertyEditor">
       </propertyEditor>
    
       <propertyEditor
             factory="de.fujaba.properties.properties.properties.editor.OCLPropertyEditorEditor$Factory"
             type="de.fujaba.properties.OCLPropertyEditor">
