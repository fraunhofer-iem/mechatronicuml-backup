<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" name="coordination" version="12.0.0.2017041100">
  <ownedViewpoints name="coordination" modelFileExtension="coordination">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="org.muml.pim.coordination.diagram" label="CoordinationSpecification Diagram" initialisation="true" domainClass="AbstractCoordinationSpecification" enablePopupBars="true">
      <metamodel href="../../org.muml.pim/model/pim.ecore#/"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//constraint"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//instance"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//protocol"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//realtimestatechart"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//msgtype"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//types"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//connector"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//valuetype"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//behavior"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//component"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//pattern"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//runnable"/>
      <metamodel href="../../org.muml.pim/model/pim.ecore#//realtimestatechart/one_to_n_schemata"/>
      <layout xsi:type="description_1:OrderedTreeLayout" childrenExpression="aql:self.roles" nodeMapping="//@ownedViewpoints[name='coordination']/@ownedRepresentations[name='org.muml.pim.coordination.diagram']/@defaultLayer/@nodeMappings[name='org.muml.pim.coordination.CoordinationSpecification']"/>
      <diagramInitialisation>
        <firstModelOperations xsi:type="tool:If" conditionExpression="aql:self.roles->isEmpty() and self.roleConnector.oclIsUndefined()">
          <subModelOperations xsi:type="tool:CreateInstance" typeName="Role" referenceName="roles" variableName="role1">
            <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="role1"/>
          </subModelOperations>
          <subModelOperations xsi:type="tool:CreateInstance" typeName="Role" referenceName="roles" variableName="role2">
            <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="role2"/>
          </subModelOperations>
          <subModelOperations xsi:type="tool:CreateInstance" typeName="RoleConnector" referenceName="roleConnectors" variableName="connector">
            <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="role1"/>
            <subModelOperations xsi:type="tool:SetValue" featureName="connectorEndpoints" valueExpression="aql:OrderedSet{role1, role2}"/>
            <subModelOperations xsi:type="tool:CreateInstance" typeName="ConnectorQualityOfServiceAssumptions" referenceName="connectorQOSAssumptions" variableName="qos"/>
          </subModelOperations>
        </firstModelOperations>
      </diagramInitialisation>
      <defaultLayer name="Default">
        <nodeMappings name="org.muml.pim.coordination.CoordinationSpecification" label="Coordination Specification" semanticCandidatesExpression="aql:self" domainClass="AbstractCoordinationSpecification">
          <style xsi:type="style:EllipseNodeDescription" borderSizeComputationExpression="2" borderLineStyle="dash" labelSize="12" showIcon="false" sizeComputationExpression="10" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelFormat>bold</labelFormat>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_blue']"/>
          </style>
          <conditionnalStyles predicateExpression="aql:self.oclIsKindOf(pattern::AbstractCoordinationPattern)">
            <style xsi:type="style:LozengeNodeDescription" borderSizeComputationExpression="2" borderLineStyle="dash" showIcon="false" sizeComputationExpression="10" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_yellow']"/>
            </style>
          </conditionnalStyles>
        </nodeMappings>
        <nodeMappings name="org.muml.pim.coordination.Role" label="Role" semanticCandidatesExpression="aql:self.roles" doubleClickDescription="//@ownedViewpoints[name='coordination']/@ownedRepresentations[name='org.muml.pim.coordination.diagram']/@defaultLayer/@toolSections.0/@ownedTools[name='roleDoubleClick']" domainClass="Role">
          <style xsi:type="style:WorkspaceImageDescription" borderSizeComputationExpression="1" labelSize="12" showIcon="false" sizeComputationExpression="2" workspacePath="/coordination.design/images/right-arrow-black.svg">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="org.muml.pim.coordination.AssumptionsEdge" label="Assumptions Edge" sourceMapping="//@ownedViewpoints[name='coordination']/@ownedRepresentations[name='org.muml.pim.coordination.diagram']/@defaultLayer/@edgeMappings[name='org.muml.pim.coordination.Role%20Connector']" targetMapping="//@ownedViewpoints[name='coordination']/@ownedRepresentations[name='org.muml.pim.coordination.diagram']/@defaultLayer/@containerMappings[name='ConnectorQualityOfServiceAssumptions']" targetFinderExpression="aql:self.connectorQualityOfServiceAssumptions">
          <style lineStyle="dot" targetArrow="NoDecoration" sizeComputationExpression="0">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="org.muml.pim.coordination.Role Connector" label="Role Connector" semanticCandidatesExpression="aql:self.roleConnector" sourceMapping="//@ownedViewpoints[name='coordination']/@ownedRepresentations[name='org.muml.pim.coordination.diagram']/@defaultLayer/@nodeMappings[name='org.muml.pim.coordination.Role']" targetMapping="//@ownedViewpoints[name='coordination']/@ownedRepresentations[name='org.muml.pim.coordination.diagram']/@defaultLayer/@nodeMappings[name='org.muml.pim.coordination.Role']" targetFinderExpression="aql:self.connectorEndpoints->last()" sourceFinderExpression="aql:self.connectorEndpoints->first()" domainClass="RoleConnector" useDomainElement="true">
          <style targetArrow="NoDecoration" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <centerLabelStyleDescription>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="org.muml.pim.coordination.CoordinationToRoleEdge" label="Coordination To Role Edge" sourceMapping="//@ownedViewpoints[name='coordination']/@ownedRepresentations[name='org.muml.pim.coordination.diagram']/@defaultLayer/@nodeMappings[name='org.muml.pim.coordination.Role']" targetMapping="//@ownedViewpoints[name='coordination']/@ownedRepresentations[name='org.muml.pim.coordination.diagram']/@defaultLayer/@nodeMappings[name='org.muml.pim.coordination.CoordinationSpecification']" targetFinderExpression="aql:self.eContainer()">
          <style lineStyle="dash" targetArrow="NoDecoration" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <centerLabelStyleDescription>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="ConnectorQualityOfServiceAssumptions" label="QOS Assumptions" semanticCandidatesExpression="aql:self.roleConnector.connectorQualityOfServiceAssumptions" domainClass="ConnectorQualityOfServiceAssumptions" childrenPresentation="List">
          <subNodeMappings name="ConnectorQualityOfServiceAssumptions" label="Reliable" semanticCandidatesExpression="aql:self" domainClass="ConnectorQualityOfServiceAssumptions">
            <style xsi:type="style:SquareDescription" labelSize="12" showIcon="false" labelExpression="aql:'Reliable: '.concat((not self.messageLossPossible).toString()).concat('\nMaxDelay: 0ms').concat('\nPreserveMsgOrder: ').concat(self.preserveMessageOrder.toString())" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_yellow']"/>
            </style>
          </subNodeMappings>
          <style xsi:type="style:FlatContainerStyleDescription" borderSizeComputationExpression="1" showIcon="false" labelExpression="QoS Assumptions:" roundedCorner="true">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelFormat>bold</labelFormat>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_yellow']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='yellow']"/>
          </style>
        </containerMappings>
        <toolSections name="org.muml.pim.coordination.createSection" label="Create">
          <ownedTools xsi:type="tool_1:NodeCreationDescription" name="org.muml.pim.coordination.createRole" label="Create Role" nodeMappings="//@ownedViewpoints[name='coordination']/@ownedRepresentations[name='org.muml.pim.coordination.diagram']/@defaultLayer/@nodeMappings[name='org.muml.pim.coordination.Role']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="Role" referenceName="roles">
                  <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="role"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:EdgeCreationDescription" name="org.muml.pim.coordination.RoleConnector" label="Create Role Connector" edgeMappings="//@ownedViewpoints[name='coordination']/@ownedRepresentations[name='org.muml.pim.coordination.diagram']/@defaultLayer/@edgeMappings[name='org.muml.pim.coordination.Role%20Connector']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:source.eContainer()">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="RoleConnector" referenceName="roleConnector" variableName="roleConnector">
                  <subModelOperations xsi:type="tool:SetValue" featureName="connectorEndpoints" valueExpression="aql:OrderedSet{source, target}"/>
                  <subModelOperations xsi:type="tool:CreateInstance" typeName="ConnectorQualityOfServiceAssumptions" referenceName="connectorQualityOfServiceAssumptions" variableName="qos"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:DoubleClickDescription" name="roleDoubleClick" mappings="//@ownedViewpoints[name='coordination']/@ownedRepresentations[name='org.muml.pim.coordination.diagram']/@defaultLayer/@nodeMappings[name='org.muml.pim.coordination.Role']">
            <element name="element"/>
            <elementView name="elementView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:element">
                <subModelOperations xsi:type="tool:If" conditionExpression="aql:self.behavior = null">
                  <subModelOperations xsi:type="tool:CreateInstance" typeName="RealtimeStatechart" referenceName="containedBehavior" variableName="rtsc"/>
                  <subModelOperations xsi:type="tool:SetValue" featureName="behavior" valueExpression="aql:rtsc"/>
                </subModelOperations>
                <subModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:self.behavior">
                  <subModelOperations xsi:type="tool_1:Navigation" createIfNotExistent="true">
                    <diagramDescription href="../../realtimestatechart.design/description/realtimestatechart.odesign#//@ownedViewpoints[name='realtimestatechart']/@ownedRepresentations[name='org.muml.pim.realtimestatechart.diagram']"/>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="coordination.design.Services"/>
  </ownedViewpoints>
</description:Group>
