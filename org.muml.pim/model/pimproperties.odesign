      </groups>
      <groups name="org.muml.pim.realtimestatechart.EffectPage" labelExpression="Properties" semanticCandidateExpression="var:self">
        <controls xsi:type="properties-ext-widgets-reference:ExtReferenceDescription" labelExpression="Events" referenceNameExpression="events"/>
      </groups>
