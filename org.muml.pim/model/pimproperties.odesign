      <pages name="org.muml.pim.realtimestatechart.TransitionConditionPage" labelExpression="Condition" domainClass="Transition" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.1"/>
      <pages name="org.muml.pim.realtimestatechart.TransitionDeadlinePage" labelExpression="Deadline" domainClass="Transition" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.2"/>
