<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension
        point="org.eclipse.ui.menus">
     <menuContribution
           locationURI="popup:org.eclipse.ui.popup.any?after=additions">
        <menu id="de.fujaba" label="Fujaba">
           <command
                 commandId="de.fujaba.core.ui.command.modelrefresh"
                 disabledIcon="icons/refresh_nav_dis.gif"
                 icon="icons/refresh_nav.gif"
                 id="de.fujaba.core.ui.command.modelrefresh"
                 label="Refresh Models"
                 style="push">
              <visibleWhen
                    checkEnabled="false">
                 <iterate
                       ifEmpty="true"
                       operator="or">
                 </iterate>
              </visibleWhen>
           </command>
           <separator
                 name="additions">
           </separator>
        </menu>
     </menuContribution>
  </extension>
  <extension
        point="org.eclipse.ui.commands">

     <command
           categoryId="de.fujaba"
           defaultHandler="de.uni_paderborn.fujaba.muml.utilities.modelrefresh.ui.ModelRefreshCommand"
           description="Refresh Models"
           id="de.fujaba.core.ui.command.modelrefresh"
           name="Refresh Models">
     </command>
  </extension>
  
</plugin>
