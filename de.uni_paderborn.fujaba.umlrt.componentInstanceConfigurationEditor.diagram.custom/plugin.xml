      </editor>
   </extension>
   

   <extension point="org.eclipse.ui.newWizards" id="creation-wizard">
  	  <wizard
  	       category="de.uni_paderborn.fujaba/de.uni_paderborn.fujaba.diagrams"
  	       icon="icons/ComponentInstance.gif"
  	       class="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram.custom.part.ComponentInstanceConfigurationDiagramCreationWizard"
  	       id="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram.custom.part.ComponentInstanceConfigurationDiagramCreationWizard"
  	       name="ComponentInstance-Configuration Diagram">
  	     
  	  	 <description>Creates a new ComponentInstance-Configuration Diagram.</description>
      </wizard>
   </extension>
   
   
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
      <editpartProvider
            class="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram.custom.providers.CustomComponentInstanceConfigurationEditPartProvider">
         <Priority
               name="Low">
         </Priority>
      </editpartProvider>
   </extension>
   
    
   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypes" id="element-types">
      <metamodel nsURI="http:///de/uni_paderborn/fujaba/umlrt/model/component">
      
        <adviceBinding  id="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram.custom.ComponentInstanceAdvice"
		  typeId="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram.ComponentInstance_2002"
		  class="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram.custom.edit.helpers.ComponentInstanceEditHelperAdvice"
		  inheritance="all">
        </adviceBinding>

      </metamodel>
   </extension>
   
   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypeBindings" id="element-types-bindings">
      <binding context="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram.TypeContext">
         <elementType ref="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram.ComponentInstance_2002"/>
         <advice ref="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram.custom.ComponentInstanceAdvice"/>
      </binding>
   </extension>


  <extension point="org.eclipse.ui.views.properties.tabbed.propertySections" id="propsections">
      <?gmfgen generated="true"?>   
      <propertySections contributorId="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram">
         <propertySection
            id="property.section.domain" 
            tab="property.tab.domain"
            class="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram.custom.sheet.CustomComponentInstanceConfigurationPropertySection">
            <input type="org.eclipse.gmf.runtime.notation.View"/>
            <input type="org.eclipse.gef.EditPart"/>
            <input type="de.uni_paderborn.fujaba.umlrt.componentInstanceConfiguration.diagram.navigator.ComponentInstanceConfigurationAbstractNavigatorItem"/>
         </propertySection>
      </propertySections>
   </extension>


