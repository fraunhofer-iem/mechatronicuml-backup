         id="de.uni_paderborn.fujaba.umlrt.componentInstanceConfiguration.diagram.part.ComponentInstanceConfigurationDiagramEditorID"
         name="%editorName"
         icon="icons/ComponentInstance.gif"
         extensions="componentinstanceconfiguration_diagram"
         default="true"
         class="de.uni_paderborn.fujaba.umlrt.componentInstanceConfigurationEditor.diagram.custom.part.CustomComponentInstanceConfigurationDiagramEditor"
         matchingStrategy="de.uni_paderborn.fujaba.umlrt.componentInstanceConfiguration.diagram.part.ComponentInstanceConfigurationMatchingStrategy"
