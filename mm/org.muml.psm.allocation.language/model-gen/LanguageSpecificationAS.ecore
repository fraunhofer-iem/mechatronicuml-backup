      <details key="constraints" value="exactlyOnePair"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="exactlyOnePair" value="self.tupleDescriptor.typedPairs->size() = 1"/>
