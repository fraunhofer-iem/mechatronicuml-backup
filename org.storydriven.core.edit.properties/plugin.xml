<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>


  <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
    <propertyContributor
          contributorId="de.uni_paderborn.fujaba.muml.common.properties">
       <propertyCategory
             category="domain">
       </propertyCategory>
       <propertyCategory
             category="default">
       </propertyCategory>
       <propertyCategory
             category="extended">
       </propertyCategory>
       <propertyCategory
             category="advanced">
       </propertyCategory>
    </propertyContributor>
  </extension>



<extension point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs contributorId="de.uni_paderborn.fujaba.muml.common.properties">
          <propertyTab
             category="domain"
             id="property.tab.general"
             label="General"/>
          <propertyTab
             category="domain"
             id="property.tab.documentation"
             afterTab="property.tab.general"
             label="Documentation"/>
          <propertyTab
             category="domain"
             id="property.tab.extensions"
             label="Extensions"/>
          
      </propertyTabs>
  </extension>




  <extension point="org.eclipse.ui.views.properties.tabbed.propertySections"> 
      <propertySections contributorId="de.uni_paderborn.fujaba.muml.common.properties">
          <propertySection
            class="de.uni_paderborn.fujaba.properties.runtime.sections.ObjectPropertySection"
            filter="org.storydriven.core.properties.tabs.GeneralTabElementFilter"
            id="property.tab.general"
            tab="property.tab.general">
             <input
               type="org.eclipse.emf.ecore.EObject">
             </input>
          </propertySection>
          <propertySection
            class="de.uni_paderborn.fujaba.properties.runtime.sections.ObjectPropertySection"
            filter="org.storydriven.core.properties.tabs.DocumentationTabElementFilter"
            id="property.tab.documentation"
            tab="property.tab.documentation">
             <input
               type="org.eclipse.emf.ecore.EObject">
             </input>
          </propertySection>
          <propertySection
            class="de.uni_paderborn.fujaba.properties.runtime.sections.ObjectPropertySection"
            filter="org.storydriven.core.properties.tabs.ExtensionsTabElementFilter"
            id="property.tab.extensions"
            tab="property.tab.extensions">
             <input
               type="org.eclipse.emf.ecore.EObject">
             </input>
          </propertySection>
         
      </propertySections>
  </extension>



 <extension point="de.uni_paderborn.fujaba.properties.runtime.propertyEditors">
    <propertyEditors>
    
       <propertyEditor
             factory="org.storydriven.core.properties.expressions.editor.TextualExpressionEditor$Factory"
             type="http://www.storydriven.org/core/expressions/0.3.1#TextualExpression">
       </propertyEditor>
    
       <propertyEditor
             factory="org.storydriven.core.properties.common.editor.UnaryExpressionEditor$Factory"
             type="http://www.storydriven.org/core/expressions/common/0.3.1#UnaryExpression">
       </propertyEditor>
    
       <propertyEditor
             factory="org.storydriven.core.properties.common.editor.ComparisonExpressionEditor$Factory"
             type="http://www.storydriven.org/core/expressions/common/0.3.1#ComparisonExpression">
       </propertyEditor>
    
       <propertyEditor
             factory="org.storydriven.core.properties.common.editor.ArithmeticExpressionEditor$Factory"
             type="http://www.storydriven.org/core/expressions/common/0.3.1#ArithmeticExpression">
       </propertyEditor>
    
       <propertyEditor
             factory="org.storydriven.core.properties.common.editor.LogicalExpressionEditor$Factory"
             type="http://www.storydriven.org/core/expressions/common/0.3.1#LogicalExpression">
       </propertyEditor>
    
       <propertyEditor
             factory="org.storydriven.core.properties.common.editor.LiteralExpressionEditor$Factory"
             type="http://www.storydriven.org/core/expressions/common/0.3.1#LiteralExpression">
       </propertyEditor>
    
    </propertyEditors>
 </extension>

</plugin>
