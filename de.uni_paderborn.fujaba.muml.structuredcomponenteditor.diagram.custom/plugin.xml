<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   

   
   
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
      <editpartProvider
            class="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.custom.providers.CustomStructuredcomponentEditPartProvider">
         <Priority
               name="Low">
         </Priority>
      </editpartProvider>
   </extension>
   
    
   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypes" id="element-types">
      <metamodel nsURI="http://www.fujaba.de/muml/model/component/0.3.1">
      
        <adviceBinding  id="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.custom.ComponentPartAdvice"
		  typeId="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.ComponentPart_3005"
		  class="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.custom.edit.helpers.ComponentPartEditHelperAdvice"
		  inheritance="all">
        </adviceBinding>
        <adviceBinding  id="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.custom.AssemblyConnectorAdvice"
		  typeId="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.AssemblyConnector_4001"
		  class="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.custom.edit.helpers.AssemblyConnectorEditHelperAdvice"
		  inheritance="all">
        </adviceBinding>
         <adviceBinding  id="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.custom.DelegationConnectorAdvice"
		  typeId="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.DelegationConnector_4002"
		  class="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.custom.edit.helpers.DelegationConnectorEditHelperAdvice"
		  inheritance="all">
        </adviceBinding>

      </metamodel>
   </extension>
   
   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypeBindings" id="element-types-bindings">
      <binding context="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.TypeContext">
         <elementType ref="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.ComponentPart_3005"/>
         <advice ref="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.custom.ComponentPartAdvice"/>
      </binding>
      <binding context="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.TypeContext">
         <elementType ref="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.AssemblyConnector_4001"/>
         <advice ref="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.custom.AssemblyConnectorAdvice"/>
      </binding>
      <binding context="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.TypeContext">
         <elementType ref="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.DelegationConnector_4002"/>
         <advice ref="de.uni_paderborn.fujaba.muml.structuredcomponenteditor.diagram.custom.DelegationConnectorAdvice"/>
      </binding>
   </extension>

