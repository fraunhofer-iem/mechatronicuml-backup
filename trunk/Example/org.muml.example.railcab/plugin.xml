<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.eclipse.ui.Examples/org.muml.example"
            class="org.muml.core.example.ui.wizard.ExampleCreationWizard"
            icon="images/muml.png"
            id="org.muml.example.ui.RailCabCreationWizard"
            name="%muml.example.railcab.name"
            projectName="%muml.example.railcab.projectName"
            projectPageDescription="%muml.example.railcab.projectPage.desc"
            projectPageTitle="%muml.example.railcab.projectPage.title">
             <source
             	bundle = "org.muml.example.railcab"
            	path = "data"
            	/>
             <description>
                %muml.example.railcab.desc
             </description>
      </wizard>
   </extension>
</plugin>
