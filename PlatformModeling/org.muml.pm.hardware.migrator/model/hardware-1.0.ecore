          lowerBound="1" eType="#//hwresource/CommunicationResource"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="connectedMediaPart" upperBound="-1"
          eType="#//hwplatform/CommunicationMediaPart" volatile="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The communication media to which this HWPortPart is connected."/>
        </eAnnotations>
        <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL">
          <details key="derivation" value="if (self.protocol.oclIsKindOf(hwresource::BusProtocol)) then&#xA;&#x9;self.connectors.oclAsType(hwplatform::BusConnector).connectedBusPart.oclAsType(hwplatform::CommunicationMediaPart)->asOrderedSet()&#xA;else &#xA;&#x9;self.connectors.oclAsType(hwplatform::CommunicationMediaPart)->asOrderedSet()&#xA;endif"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="protocol" eType="#//hwresource/CommunicationProtocol"
          changeable="false" volatile="true" transient="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL">
          <details key="derivation" value="if(self.communicationResource.oclIsUndefined()) then&#xA;&#x9;null &#xA;else&#xA; self.communicationResource.protocol&#xA; endif"/>
        </eAnnotations>
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The used protocol of this HWPortInstance."/>
        </eAnnotations>
      </eStructuralFeatures>
