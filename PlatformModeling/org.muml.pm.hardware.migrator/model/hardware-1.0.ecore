        </eAnnotations>
        <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL">
          <details key="derivation" value="if(self.communicationResource.oclIsUndefined()) then&#xA;&#x9; null&#xA;&#x9;else&#xA;&#x9; &#x9;self.communicationResource.cardinality&#xA;&#x9; endif"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="multiHWPort" lowerBound="1"
