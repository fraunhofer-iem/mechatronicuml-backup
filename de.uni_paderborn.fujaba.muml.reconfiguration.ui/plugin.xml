       <extension
             point="de.uni_paderborn.fujaba.modelinstance.ui.diagraminformation">
          <editor
                editorId="de.uni_paderborn.fujaba.muml.component.diagram.part.ComponentDiagramEditorID"
                editorName="Component Editor"
                fileExtension="component_diagram"
                modelElementCategoryKey="de.uni_paderborn.fujaba.muml.components.category"
                modelId="Component"
                preferencesHint="de.uni_paderborn.fujaba.muml.component.diagram">
             <diagramElement
                   packageNsURI="http://www.fujaba.de/modelinstance/0.4.0"
                   className="ModelElementCategory">
             </diagramElement>
             <topLevelNode
                   domainElement="ReconfigurableStructuredComponent"
                   semanticHint="2076">
             </topLevelNode>
          </editor>
       </extension>
       <extension
             point="org.eclipse.gmf.runtime.diagram.core.viewProviders">
          <viewProvider
                class="de.uni_paderborn.fujaba.muml.reconfiguration.ui.providers.ReconfigurationViewProvider">
             <Priority
                   name="Low">
             </Priority>
            
          </viewProvider>
       </extension>
</plugin>
