<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>


<extension point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs contributorId="de.uni_paderborn.fujaba.muml.common.properties">
          <propertyTab
             category="domain"
             id="base"
             label="Experimental"/>
          
      </propertyTabs>

</extension>

 <extension point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections contributorId="de.uni_paderborn.fujaba.muml.common.properties">
         <propertySection
               class="de.fujaba.properties.runtime.sections.ObjectPropertySection"
               id="State"
               tab="base">
            <input
                  type="de.uni_paderborn.fujaba.muml.realtimestatechart.State">
            </input>
         </propertySection>
      </propertySections>
 </extension>
 
 <extension
       point="de.fujaba.properties.runtime.propertyEditor">
    <propertyEditors>
       <propertyEditor
             editor="test.editor.StateEditor"
             tab="base"
             type="de.uni_paderborn.fujaba.muml.realtimestatechart.State">
       </propertyEditor>
    </propertyEditors>
 </extension>

</plugin>
