<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

<!-- FOREACH tabs -->
<extension point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs contributorId="de.uni_paderborn.fujaba.muml.common.properties">
          <propertyTab
             category="domain"
             id="base"
             label="Experimental"/>
          
      </propertyTabs>

</extension>

<!-- (also one section per tab) -->
<extension
      point="org.eclipse.ui.views.properties.tabbed.propertySections">
   <propertySections
         contributorId="de.uni_paderborn.fujaba.muml.common.properties">
      <propertySection
            class="de.fujaba.properties.runtime.sections.ObjectPropertySection"
            filter="de.fujaba.properties.runtime.filter.ElementFilter"
            id="base"
            tab="base">
         <input
               type="org.eclipse.emf.ecore.EObject">
         </input>
      </propertySection>
   </propertySections>
</extension>
<!-- ENDFOREACH -->
 
 <extension
       point="de.fujaba.properties.runtime.propertyEditor">
    <propertyEditors>
       <propertyEditor
             editor="test.editor.StateEditor"
             tab="base"
             type="de.uni_paderborn.fujaba.muml.realtimestatechart.State">
       </propertyEditor>
    </propertyEditors>
 </extension>


 
 <extension
       point="de.fujaba.properties.runtime.propertyEditor">
    <propertyEditors>
       <propertyEditor
             editor="test.editor.NamedElementEditor"
             tab="base"
             type="org.storydriven.core.NamedElement">
       </propertyEditor>
    </propertyEditors>
 </extension>

</plugin>
