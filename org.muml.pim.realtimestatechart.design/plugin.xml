<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
  <extension point="org.eclipse.sirius.componentization">
    <component class="realtimestatechart.design.Activator"
               id="realtimestatechart.design"
	       name="realtimestatechart">
    </component>
  </extension>
  <!--
  <extension point="org.eclipse.sirius.diagram.styleConfigurationProvider">
  	<styleConfigurationProvider providerClass="com.example.diagseq.style.DiagSeqStyleConfigurationProvider" />
  </extension>
  -->
  <extension point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
		<editpartProvider class="realtimestatechart.design.providers.MyEditPartProvider">
       		<Priority name="High"/>
	    </editpartProvider>
  </extension>
  <extension
        point="org.eclipse.gmf.runtime.diagram.ui.editpolicyProviders">
     <editpolicyProvider
           class="realtimestatechart.design.providers.MyEditPolicyProvider">
        <Priority
              name="Lowest">
        </Priority>
     </editpolicyProvider>
  </extension>
  <extension
        point="org.eclipse.gmf.runtime.diagram.core.viewProviders">
     <viewProvider
           class="realtimestatechart.design.providers.MyViewProvider">
        <Priority
              name="High">
        </Priority>
     </viewProvider>
  </extension>
  <extension
        point="org.eclipse.gmf.runtime.diagram.ui.decoratorProviders">
     <decoratorProvider
           class="realtimestatechart.design.providers.MyDecoratorProvider">
        <Priority
              name="High">
        </Priority>
     </decoratorProvider>
  </extension>
</plugin>
