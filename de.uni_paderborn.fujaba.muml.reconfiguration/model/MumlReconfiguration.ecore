    <eStructuralFeatures xsi:type="ecore:EReference" name="timeForExecution" lowerBound="1"
        eType="ecore:EClass ../../de.uni_paderborn.fujaba.muml/model/muml.ecore#//valuetype/TimeValue"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ExecutionTimingSpecificationThreePhase"
