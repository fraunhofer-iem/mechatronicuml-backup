<?xml version="1.0" encoding="UTF-8"?>
<de.uni_paderborn.fujaba.muml.testlanguage:TestCase xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:de.uni_paderborn.fujaba.muml.testlanguage="http://de/uni_paderborn/fujaba/muml/testlanguage" xmlns:de.uni_paderborn.fujaba.muml.testlanguage.specification="http://de/uni_paderborn/fujaba/muml/testlanguage/specification">
  <nodes label="Load Model" specificationClassName="LoadModel">
    <outputs name="model" targets="//@nodes.1/@inputs[name='muml']"/>
    <specification xsi:type="de.uni_paderborn.fujaba.muml.testlanguage.specification:LoadModel" label="Load Model" uri="platform:/resource/de.uni_paderborn.fujaba.muml.testlanguage.paper/models/overtaking-cars_correct-protocols/overtaking_Correct.muml">
      <portSpecifications name="model" optional="true" type="OUT"/>
    </specification>
  </nodes>
  <nodes label="Check MTCTL" specificationClassName="CheckMTCTL">
    <inputs name="muml" source="//@nodes.0/@outputs[name='model']" dataType="de.uni_paderborn.fujaba.modelinstance.RootNode"/>
    <inputs name="protocol" source="//@nodes.2/@outputs[name='string']" optional="true" dataType="java.lang.String"/>
    <inputs name="options" optional="true" dataType="de.uni_paderborn.fujaba.muml.verification.uppaal.options.Options"/>
    <outputs name="results" targets="//@nodes.3/@inputs[name='property_result_repository']"/>
    <specification xsi:type="de.uni_paderborn.fujaba.muml.testlanguage.specification:CheckMTCTL" label="Check MTCTL" properties="AG (stateActive(rear.overtaking) implies stateActive(front.noAcceleration)); //the following are default properties&#xD;&#xA;AG not deadlock; //a deadlock never occurs&#xD;&#xA;AG not bufferOverflow; //a buffer overflow never occurs&#xD;&#xA;forall(s : States) EF stateActive(s); //there is no state that can never be active&#xD;&#xA;forall(m : MessageTypes) exists(b : Buffers) EF messageInBuffer(m, b); //there is no message type that cannot possibly arrive in a buffer&#xD;&#xA;forall(m : MessageTypes) EF messageInTransit(m); //there is no message type that cannot possibly be in transit&#xD;&#xA;forall(b : Buffers) EF bufferMessageCount(b) >= 1; //there is no buffer that is always empty">
      <portSpecifications name="muml" dataType="de.uni_paderborn.fujaba.modelinstance.RootNode"/>
      <portSpecifications name="protocol" optional="true" dataType="java.lang.String"/>
      <portSpecifications name="options" optional="true" dataType="de.uni_paderborn.fujaba.muml.verification.uppaal.options.Options"/>
      <portSpecifications name="results" type="OUT"/>
    </specification>
  </nodes>
  <nodes label="Primitive String" specificationClassName="PrimitiveString">
    <outputs name="string" targets="//@nodes.1/@inputs[name='protocol']"/>
    <specification xsi:type="de.uni_paderborn.fujaba.muml.testlanguage.specification:PrimitiveString" label="Primitive String" value="Overtaking">
      <portSpecifications name="string" type="OUT"/>
    </specification>
  </nodes>
  <nodes label="Assert Result" specificationClassName="AssertResult">
    <inputs name="property_result_repository" source="//@nodes.1/@outputs[name='results']" dataType="de.uni_paderborn.fujaba.muml.verification.uppaal.results.PropertyResultRepository"/>
    <specification xsi:type="de.uni_paderborn.fujaba.muml.testlanguage.specification:AssertResult" label="Assert Result">
      <portSpecifications name="property_result_repository" dataType="de.uni_paderborn.fujaba.muml.verification.uppaal.results.PropertyResultRepository"/>
    </specification>
  </nodes>
</de.uni_paderborn.fujaba.muml.testlanguage:TestCase>
