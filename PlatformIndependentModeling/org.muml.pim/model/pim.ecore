      <eStructuralFeatures xsi:type="ecore:EReference" name="statechart" lowerBound="1"
          eType="#//realtimestatechart/RealtimeStatechart" changeable="false" volatile="true"
          transient="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The realtime statechart that this transition locically belongs to."/>
