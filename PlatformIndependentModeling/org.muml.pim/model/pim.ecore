        <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL">
          <details key="derivation" value="self -> closure(&#xD;&#xA;&#x9;if parentRegion.oclIsUndefined() then &#xD;&#xA;&#x9;&#x9;self &#xD;&#xA;&#x9;else &#xD;&#xA;&#x9;&#x9;parentRegion.parentState.parentStatechart &#xD;&#xA;&#x9;endif&#xD;&#xA;).clocks->asSet()"/>
