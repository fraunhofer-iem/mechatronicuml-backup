<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.muml.modelica.adapter.feature"
      label="MechatronicUML Modelica Adapter"
      version="1.0.0.qualifier"
      provider-name="%providerName">

   <description>
      Modelica code generation.
   </description>

   <copyright url="%copyrightURL">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import feature="org.muml.pim.feature" version="0.5.0.qualifier"/>
      <import feature="org.muml.graphviz.feature" version="0.5.0.qualifier"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.muml.pim"/>
      <import plugin="org.muml.pim.actionlanguage"/>
      <import plugin="org.muml.core"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.emf.ecore.xmi"/>
      <import plugin="org.eclipse.ocl"/>
      <import plugin="org.eclipse.ocl.ecore"/>
      <import plugin="org.eclipse.acceleo.common" version="3.2.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.acceleo.model" version="3.2.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.acceleo.profiler" version="3.2.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.acceleo.engine" version="3.2.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.acceleo.parser" version="3.2.2" match="greaterOrEqual"/>
      <import plugin="org.muml.modelica.transform" version="0.4.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.m2m.qvt.oml.runtime"/>
      <import plugin="org.eclipse.m2m.qvt.oml.project"/>
      <import plugin="org.muml.graphviz.blackbox" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.m2m.qvt.oml" version="3.3.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.muml.core.modelinstance.ui" version="0.4.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.cdo"/>
      <import plugin="org.eclipse.emf.edit"/>
      <import plugin="org.muml.core.edit"/>
      <import plugin="org.eclipse.emf.ecore.edit"/>
      <import plugin="org.muml.pim.edit"/>
      <import feature="org.muml.core.feature" version="0.5.0"/>
      <import plugin="org.muml.modelica.transform.edit" version="0.4.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.muml.modelica.adapter"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.muml.modelica.adapter.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
