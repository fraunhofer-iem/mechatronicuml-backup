[module time('http://www.fujaba.de/muml/valuetype/0.4.0')]

[**
 * Returns a factor for the time value's unit.
 * @param timeValue the time value
*/]
[query public getTimeUnitFactor(timeValue : TimeValue) : Real =
	invoke('de.uni_paderborn.fujaba.modelica.m2t.rtsc.util.query.TimeHelper',
		'getFactor(de.uni_paderborn.fujaba.muml.valuetype.TimeValue)',
		Sequence{timeValue})
/]