<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="de.uni_paderborn.fujaba/de.uni_paderborn.fujaba.diagrams"
            class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.part.CustomRealtimeStatechartDiagramCreationWizard"
            icon="icons/StatechartFile.gif"
            id="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.realtimestatechartwizard"
            name="Realtime Statechart Diagram">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.part.CustomRealtimeStatechartEditor"
            contributorClass="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.part.RealtimeStatechartDiagramActionBarContributor"
            default="true"
            extensions="statechart_diagram"
            icon="icons/StatechartFile.gif"
            id="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.part.RealtimeStatechartDiagramEditorID"
            matchingStrategy="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.part.RealtimeStatechartMatchingStrategy"
            name="%editorName">
      </editor>
   </extension>
   <extension
         id="propSections"
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram">
         <propertySection
               class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.sheet.CustomRealtimeStatechartPropertySection"
               id="property.section.domain"
               tab="property.tab.domain">
            <input
                  type="org.eclipse.gmf.runtime.notation.View">
            </input>
            <input
                  type="org.eclipse.gef.EditPart">
            </input>
            <input
                  type="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.navigator.RealtimeStatechartAbstractNavigatorItem">
            </input>
         </propertySection>
      </propertySections>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
      <editpartProvider
            class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.providers.CustomRealtimeStatechartEditPartProvider">
         <Priority
               name="Medium">
         </Priority>
      </editpartProvider>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="objectContribution.Transition"
            objectClass="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.edit.parts.TransitionEditPart">
         <menu
               id="RealtimeStatechart.Insert"
               label="Custom.Insert"
               path="additions">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.actions.AbsoluteDeadlineCreateAction"
               enablesFor="1"
               icon="icons/clock.gif"
               id="Statechart.CreateAbsoluteDeadlineID"
               label="AbsoluteDeadline"
               menubarPath="RealtimeStatechart.Insert/group1">
         </action>
         <action
               class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.actions.RelativeDeadlineCreateAction"
               enablesFor="1"
               icon="icons/clock.gif"
               id="Statechart.CreateRelativeDeadlineID"
               label="RelativeDeadline"
               menubarPath="RealtimeStatechart.Insert/group1">
         </action>
         <action
               class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.actions.RaisedEventCreateAction"
               enablesFor="1"
               icon="icons/e_raised16.gif"
               id="Statechart.CreateRaisedEventID"
               label="RaisedEvent"
               menubarPath="RealtimeStatechart.Insert/group1">
         </action>
         <action
               class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.actions.TriggerEventCreateAction"
               enablesFor="1"
               icon="icons/e_trigger16.gif"
               id="Statechart.CreateTriggerEventID"
               label="TriggerEvent"
               menubarPath="RealtimeStatechart.Insert/group1">
         </action>
         <action
               class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.actions.ModifySynchornizationWizardAction"
               enablesFor="1"
               icon="icons/Syn.gif"
               id="Statechart.CreateSynchroID"
               label="ModifySynchronization"
               menubarPath="RealtimeStatechart.Insert/group1">
         </action>
         <action
               class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.actions.TransitionActionCreateAction"
               enablesFor="1"
               icon="icons/setRefMethod.gif"
               id="Statechart.CreateTransitionActionID"
               label="TransitionAction"
               menubarPath="RealtimeStatechart.Insert/group1">
         </action>
         <action
               class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.actions.TimeGuardCreateAction"
               enablesFor="1"
               icon="icons/clock.gif"
               id="Statechart.CreateTimeGuardID"
               label="TimeGuard"
               menubarPath="RealtimeStatechart.Insert/group1">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="objectContribution.SynchronizationChannel"
            objectClass="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.edit.parts.SynchronizationChannelEditPart">
         <menu
               id="RealtimeStatechart.CreateEParameter"
               label="Custom.Insert"
               path="additions">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.actions.ModifySyncChannelWizardAction"
               enablesFor="1"
               id="Statechart.ModifyParameterID"
               label="Modify Parameters"
               menubarPath="RealtimeStatechart.CreateEParameter/group1">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="objectContribution.RealtimeStatechart"
            objectClass="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.edit.parts.RealtimeStatechartEditPart">
         <menu
               id="RealtimeStatechart.CreateEDataType"
               label="Custom.Insert"
               path="additions">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="de.uni_paderborn.fujaba.umlrt.realtimeStatechart.diagram.custom.actions.ModifyRTSCEDataTypesWizardAction"
               enablesFor="1"
               id="Statechart.ModifyDataTypeID"
               label="Modify DataTypes"
               menubarPath="RealtimeStatechart.CreateEDataType/group1">
         </action>
      </objectContribution>
   </extension>

</plugin>
