<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension point="org.eclipse.ui.editors" id="gmf-editor">
      <?gmfgen generated="true"?>
      <editor
         id="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.part.ComponentinstanceconfigurationDiagramEditorID"
         name="%editorName"
         icon="icons/ComponentInstanceConfDiagram.gif"
         extensions="componentinstanceconfiguration_diagram"
         default="true"
         class="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.part.ComponentinstanceconfigurationDiagramEditor"
         matchingStrategy="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.part.MumlMatchingStrategy"
         contributorClass="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.part.MumlDiagramActionBarContributor">
      </editor>
   </extension>
   

   <extension point="org.eclipse.ui.newWizards" id="creation-wizard">
  	  <wizard
  	       category="de.fujaba/de.fujaba.diagrams"
  	       icon="icons/ComponentInstanceConfDiagram.gif"
  	       class="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.custom.part.CustomComponentinstanceconfigurationDiagramCreationWizard"
  	       id="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.custom.part.CustomComponentinstanceconfigurationDiagramCreationWizard"
  	       name="ComponentInstance Configuration Diagram">
  	     
  	  	 <description>Creates a new ComponentInstance Configuration Diagram.</description>
      </wizard>
   </extension>
   
   
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
      <editpartProvider
            class="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.custom.providers.CustomComponentinstanceconfigurationEditPartProvider">
         <Priority
               name="Low">
         </Priority>
      </editpartProvider>
   </extension>
   
    
   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypes" id="element-types">
      <metamodel nsURI="http:///de/uni_paderborn/fujaba/muml/model/instance">
      
        <adviceBinding  id="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.custom.ComponentInstanceAdvice"
		  typeId="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.ComponentInstance_2004"
		  class="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.custom.edit.helpers.ComponentInstanceEditHelperAdvice"
		  inheritance="all">
        </adviceBinding>

      </metamodel>
   </extension>
   
   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypeBindings" id="element-types-bindings">
      <binding context="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.TypeContext">
         <elementType ref="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.ComponentInstance_2004"/>
         <advice ref="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.custom.ComponentInstanceAdvice"/>
      </binding>
   </extension>
   
   
   <extension point="org.eclipse.gmf.runtime.common.ui.services.parserProviders" id="parser-provider">
      <?gmfgen generated="true"?>
      <ParserProvider class="de.uni_paderborn.fujaba.muml.componentinstanceconfigurationeditor.diagram.custom.providers.CustomComponentinstanceconfigurationParserProvider">
         <Priority name="Low"/>
      </ParserProvider>
   </extension>

   <extension
         point="de.fujaba.modelinstance.modelelementcategory">
      <category
            key="de.uni_paderborn.fujaba.muml.components.category"
            name="Component Instance Configurations">
         <child
               class="ComponentInstanceConfiguration"
               packageNsUri="http:///de/uni_paderborn/fujaba/muml/model/instance">
         </child>
      </category>
   </extension>

</plugin>
