  <eClassifiers xsi:type="ecore:EClass" name="StructuredComponent" eSuperTypes="#//Component">
    <eStructuralFeatures xsi:type="ecore:EReference" name="componentParts" upperBound="-1"
        eType="#//ComponentPart" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ComponentPart">
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" lowerBound="1" eType="#//Component"/>
  </eClassifiers>
