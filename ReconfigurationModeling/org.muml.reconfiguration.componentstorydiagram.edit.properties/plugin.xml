<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>


  <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
    <propertyContributor
          contributorId="de.uni_paderborn.fujaba.muml.common.properties">
       <propertyCategory
             category="domain">
       </propertyCategory>
       <propertyCategory
             category="default">
       </propertyCategory>
       <propertyCategory
             category="extended">
       </propertyCategory>
       <propertyCategory
             category="advanced">
       </propertyCategory>
    </propertyContributor>
  </extension>



<extension point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs contributorId="de.uni_paderborn.fujaba.muml.common.properties">
          <propertyTab
             category="domain"
             id="property.tab.constraint"
             afterTab="property.tab.comment"
             label="Constraint"/>
          
      </propertyTabs>
  </extension>




  <extension point="org.eclipse.ui.views.properties.tabbed.propertySections"> 
      <propertySections contributorId="de.uni_paderborn.fujaba.muml.common.properties">
          <propertySection
            class="de.uni_paderborn.fujaba.properties.runtime.sections.ObjectPropertySection"
            filter="org.muml.reconfiguration.componentstorydiagram.properties.tabs.ConstraintTabElementFilter"
            id="property.tab.constraint"
            tab="property.tab.constraint">
             <input
               type="org.eclipse.emf.ecore.EObject">
             </input>
          </propertySection>
         
      </propertySections>
  </extension>



 <extension point="org.muml.ape.runtime.propertyEditors">
    <propertyEditors>
    
       <propertyEditor
             factory="org.muml.reconfiguration.componentstorydiagram.properties.componentstorydiagram.editor.ComponentStoryRuleEditor$Factory"
             type="http://www.fujaba.de/muml/componentstorydiagram/0.4.0#ComponentStoryRule">
       </propertyEditor>
    
       <propertyEditor
             factory="org.muml.reconfiguration.componentstorydiagram.properties.componentstorydiagram.editor.ComponentStoryNodeEditor$Factory"
             type="http://www.fujaba.de/muml/componentstorydiagram/0.4.0#ComponentStoryNode">
       </propertyEditor>
    
       <propertyEditor
             factory="org.muml.reconfiguration.componentstorydiagram.properties.componentstorydiagram.editor.ControllerExchangeNodeEditor$Factory"
             type="http://www.fujaba.de/muml/componentstorydiagram/0.4.0#ControllerExchangeNode">
       </propertyEditor>
    
       <propertyEditor
             factory="org.muml.reconfiguration.componentstorydiagram.properties.componentstorydiagram.editor.SendReconfigurationMessageExpressionEditor$Factory"
             type="http://www.fujaba.de/muml/componentstorydiagram/0.4.0#SendReconfigurationMessageExpression">
       </propertyEditor>
    
    </propertyEditors>
 </extension>


 <extension
       point="org.muml.ape.runtime.metamodelContributor">
    <metamodel
          contributor="org.muml.reconfiguration.componentstorydiagram.properties.metamodel.MetamodelContributor">
    </metamodel>
 </extension>


 <extension
       point="org.muml.ape.runtime.creationConstraintContributor">
    <constraint
          contributor="org.muml.reconfiguration.componentstorydiagram.properties.constraint.CreationConstraintContributor">
    </constraint>
 </extension>

</plugin>
