    <path id="org.muml.container.codegen.c.classpath">
        <path refid="org.muml.container.codegen.c.libraryclasspath"/>
    </path>

    <!-- The classpath with only the jars available in the plugin folder of Eclipse -->
    <path id="org.muml.container.codegen.c.libraryclasspath.alljars">
        <pathelement location="../org.muml.container.codegen.c/bin"/>
        <fileset dir="${ECLIPSE_HOME}/plugins/" includes="*.jar"/>
    </path>

    <path id="org.muml.container.codegen.c.classpath.alljars">
        <path refid="org.muml.container.codegen.c.libraryclasspath.alljars"/>
    </path>

    <!-- The default task with the jars used by this project -->
    <target name="main">
        <java classname="org.muml.container.codegen.c.Main" classpathref="org.muml.container.codegen.c.classpath">
            <arg value="${model}"/>
            <arg value="${target}"/>
        </java>
    </target>

    <!--
        In case of problem you can also try with all the jar available in your Eclipse
        Keep in mind that having two versions of the same jar can create problems
    -->
    <target name="main.alljars">
        <java classname="org.muml.container.codegen.c.Main" classpathref="org.muml.container.codegen.c.classpath">
            <arg value="${model}"/>
            <arg value="${target}"/>
        </java>
    </target>
