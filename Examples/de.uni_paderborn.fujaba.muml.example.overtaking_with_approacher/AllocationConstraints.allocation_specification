runningExample {
	
		include 'platform:/plugin/de.uni_paderborn.fujaba.muml.allocation.language.xtext/operations/OCLContext.ocl'
		
		constraint differentLocation OvertakerDistributeCommAndDrv {
			descriptors (first, second);
			ocl self.allocateToDifferentECUs('overtakerCommunicator', 'overtakerDriver');
		}
		constraint differentLocation OvertakeeDistributeCommAndDrv {
		descriptors (first, second);
		ocl self.allocateToDifferentECUs('overtakeeCommunicator', 'overtakeeDriver');
		}
		constraint differentLocation ApproacherDistributeCommAndDrv {
		descriptors (first, second);
		ocl self.allocateToDifferentECUs('approacherCommunicator', 'approacherDriver');
		}
		
	constraint requiredHardwareResourceInstance OvertakerMotorLSensor {
		descriptors (first, second);
		ocl self.allocateToECU('overtakerMotorL', 'overtakerMotor');
	}
	constraint requiredHardwareResourceInstance OvertakeeMotorLSensor {
		descriptors (first, second);
		ocl self.allocateToECU('overtakeeMotorL', 'overtakeeMotor');
	}
	constraint requiredHardwareResourceInstance ApproacherMotorLSensor {
		descriptors (first, second);
		ocl self.allocateToECU('approacherMotorL', 'approacherMotor');
	}
	constraint requiredHardwareResourceInstance OvertakerMotorRSensor {
		descriptors (first, second);
		ocl self.allocateToECU('overtakerMotorR', 'overtakerMotor');
	}
	constraint requiredHardwareResourceInstance OvertakeeMotorRSensor {
		descriptors (first, second);
		ocl self.allocateToECU('overtakeeMotorR', 'overtakeeMotor');
	}
	constraint requiredHardwareResourceInstance ApproacherMotorRSensor {
		descriptors (first, second);
		ocl self.allocateToECU('approacherMotorR', 'approacherMotor');
	}
		
	constraint requiredHardwareResourceInstance OvertakerLineSensor {
		descriptors (first, second);
		ocl self.allocateToECU('overtakerLine', 'overtakerLine');
	}
	constraint requiredHardwareResourceInstance OvertakeeLineSensor {
		descriptors (first, second);
		ocl self.allocateToECU('overtakeeLine', 'overtakeeLine');
	}
	constraint requiredHardwareResourceInstance ApproacherLineSensor {
		descriptors (first, second);
		ocl self.allocateToECU('approacherLine', 'approacherLine');
	}
		
	constraint requiredHardwareResourceInstance OvertakerDistanceSensor {
		descriptors (first, second);
		ocl self.allocateToECU('overtakerDistance', 'overtakerDistance');
	}
	constraint requiredHardwareResourceInstance OvertakeeDistanceSensor {
		descriptors (first, second);
		ocl self.allocateToECU('overtakeeDistance', 'overtakeeDistance');
	}
	constraint requiredHardwareResourceInstance ApproacherDistanceSensor {
